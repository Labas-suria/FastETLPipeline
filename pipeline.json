{
  "extr_from_txt": {
    "class": "extract",
    "type": "txt",
    "params": {
      "path": "TEST_SOURCE/TESTE.txt",
      "header_line": true,
      "first_line": 1,
      "last_line": 4,
      "separator": ";"
    }
  },
  "transf1": {
    "class": "transform",
    "type": "default",
    "params": {
      "transform_type": "remove",
      "filter_keys": ["1","a"],
      "str_regex": "^a"
    }
  },
  "csv_load": {
    "class": "load",
    "type": "csv",
    "params": {
      "file_path": "C:\\Users\\user\\Desktop\\teste.csv"
    }
  },
  "flow": "extr_from_txt->transf1->csv_load"
}
